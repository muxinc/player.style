{
  "name": "player.style",
  "version": "0.0.0",
  "description": "A fresh collection of media player themes for every use case!",
  "author": "@muxinc",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/muxinc/player.style#readme",
  "bugs": {
    "url": "https://github.com/muxinc/player.style/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muxinc/player.style.git"
  },
  "files": [
    "themes/*",
    "!.turbo/"
  ],
  "exports": {
    "./*": "./themes/*"
  },
  "workspaces": [
    "site",
    "scripts/*",
    "themes/*"
  ],
  "scripts": {
    "lint": "turbo lint",
    "test": "turbo test --concurrency=1",
    "dev": "turbo run dev --concurrency=20 --filter '!./examples/*'",
    "build": "turbo build"
  },
  "dependencies": {
    "media-chrome": "^3.2.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "turbo": "^2.0.3",
    "typescript": "^5"
  },
  "packageManager": "npm@10.8.1"
}
